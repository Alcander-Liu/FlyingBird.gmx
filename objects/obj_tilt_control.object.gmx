    case 0:
        obj_bird.hspeed += obj_bird.bird_hor_speed;
        break;
    case -1:
        obj_bird.hspeed += 2*obj_bird.bird_hor_speed;
        break;
    }
    preTiltDir = 1;
}
else
{
    switch (preTiltDir)
    {
    case 1:
        obj_bird.hspeed -= obj_bird.bird_hor_speed;
        break;
    case -1:
        obj_bird.hspeed += obj_bird.bird_hor_speed;
        break;
    }
    preTiltDir = 0;
}
